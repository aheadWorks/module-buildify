<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/awBuildifyEntity/get" method="GET">
        <service class="Aheadworks\Buildify\Api\EntityRepositoryInterface" method="get"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route url="/V1/awBuildifyProductListWidgetConfig/getProductListByWidgetConfig" method="GET">
        <service class="Aheadworks\Buildify\Api\ProductListWidgetConfigInterface" method="getProductListByWidgetConfig"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>
